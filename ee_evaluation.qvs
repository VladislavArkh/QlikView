sub AddMonths

    Мэппинг.Месяц:
    LOAD
        [Дата], 
        [Количество дней],
        [Месяц]
    from
        [$(Month_Book)]
    (ooxml, embedded labels, table is [$(Month_List)])
    where
        not isNull([Месяц]);

    monthname_to_number:mapping load distinct [Дата], Месяц Resident Мэппинг.Месяц;
end sub;

