    where [Показатель_ЭЭ_2_натуральные_производственные_показатели] = 'СЕНОМАН Низконапорные скв., м3/час'
    and [Месторождение] = 'Верхнесалымское';

    left join(Расчет_показателя_эл_мощность_temp_1)
