//2.9. Расчет тарифа на ЭЭ
sub calculate_el_tariff

    Расчет_тарифа_temp_1:
    load
    RowNo() as ID,
    [ОЕ УУ],
    [Version],
    [Scenario],
    [Показатели ТЭП] as [Тариф на покупку от сетей],
    [Входящие показатели ТЭП] as [Входящие показатели ТЭП _Тариф],
    [Входящие показатели 2 ТЭП] as [Входящие показатели 2 ТЭП _Тариф],
    [Месторождение],
    [Дата],
    applymap('monthname_to_number', [Дата]) as [Месяц _Тариф]
    Resident ТЭП_2
    where [Входящие показатели ТЭП] = 'Тариф на покупку от сетей';

    left join(Расчет_тарифа_temp_1)
    load
    [ОЕ УУ],
    [Version],
    [Scenario],
    [Показатели ТЭП] as [Тариф на вырабатываемую электроэнергию (без налогов и амортизации)],
    [Месторождение],
    [Дата],
    applymap('monthname_to_number', [Дата]) as [Месяц _Тариф]
    Resident ТЭП_2
    where
    [Входящие показатели ТЭП] = 'Тариф на вырабатываемую электроэнергию (без налогов и амортизации)';

    left join(Расчет_тарифа_temp_1)
    load
    [ОЕ УУ],
    [Version],
    [Scenario],
    [Показатели ТЭП] as [Индексации тарифа ээ],
    [Месторождение],
    [Дата],
    applymap('monthname_to_number', [Дата]) as [Месяц _Тариф]
    Resident ТЭП_2
    where
    [Входящие показатели ТЭП] = 'Индексации тарифа ээ';
    
    //Тариф на покупку от сетей
    Расчет_тарифа_temp_2:
    load
    [ОЕ УУ],
    [Version],
    [Scenario],
    [Месторождение],
    'Тариф на покупку от сетей' as [Показатель_2_ЭЭ_Тариф],
    [Входящие показатели 2 ТЭП _Тариф] as [Тариф_Входящие_показатели_2],
    [Дата],
    ([Тариф на покупку от сетей] * (1 + [Индексации тарифа ээ])) as [Параметры_Тариф]
    Resident Расчет_тарифа_temp_1;

    //Тариф на вырабатываемую электроэнергию (без налогов и амортизации)
    Concatenate(Расчет_тарифа_temp_2)
    load
    [ОЕ УУ],
    [Version],
    [Scenario],
    [Месторождение],
    'Тариф на вырабатываемую электроэнергию (без налогов и амортизации)' as [Показатель_2_ЭЭ_Тариф],
    [Входящие показатели 2 ТЭП _Тариф] as [Тариф_Входящие_показатели_2],
    [Дата],
    ([Тариф на вырабатываемую электроэнергию (без налогов и амортизации)] * (1 + [Индексации тарифа ээ])) as [Параметры_Тариф]
    Resident Расчет_тарифа_temp_1;

    //--Тариф_факт
    //--январь
    tariff_fact_january:
    noconcatenate
    load
    [Тариф на покупку от сетей] as [Тариф на покупку от сетей факт январь]
    FROM
    [$(Fact_reference_book_january_2025)]
    (ooxml, embedded labels, table is [$(Fact_reference_list)])
    where [Месторождение] = 'Западно-Салымское'
    and [Показатель ЭЭ 2] = 'Потребление электроэнергии МП';

    left join(Расчет_тарифа_temp_1)
    load
    'JAN25' as [Дата],
    applymap('monthname_to_number', 'JAN25') as [Месяц _Тариф],
    [Тариф на покупку от сетей факт январь]
    Resident tariff_fact_january;

    Concatenate(Расчет_тарифа_temp_2)
    load
    'SPD' as [ОЕ УУ],
    'WORK' as [Version],
    'FACT' as [Scenario],
    'Западно-Салымское' as [Месторождение],
    'Тариф на покупку от сетей' as [Показатель_2_ЭЭ_Тариф],
    'ФАКТ' as [Тариф_Входящие_показатели_2],
    'JAN25' as [Дата],
    applymap('monthname_to_number', 'JAN25') as [Месяц _Тариф],
    [Тариф на покупку от сетей факт январь] as [Параметры_Тариф]
    Resident tariff_fact_january;
    drop table tariff_fact_january;

    //--февраль
    tariff_fact_february:
    noconcatenate
    load
    [Тариф на покупку от сетей] as [Тариф на покупку от сетей факт февраль]
    FROM
    [$(Fact_reference_book_february_2025)]
    (ooxml, embedded labels, table is [$(Fact_reference_list)])
    where [Месторождение] = 'Западно-Салымское'
    and [Показатель ЭЭ 2] = 'Потребление электроэнергии МП';

    left join(Расчет_тарифа_temp_1)
    load
    'FEB25' as [Дата],
    applymap('monthname_to_number', 'FEB25') as [Месяц _Тариф],
    [Тариф на покупку от сетей факт февраль]
    Resident tariff_fact_february;

    Concatenate(Расчет_тарифа_temp_2)
    load
    'SPD' as [ОЕ УУ],
    'WORK' as [Version],
    'FACT' as [Scenario],
    'Западно-Салымское' as [Месторождение],
    'Тариф на покупку от сетей' as [Показатель_2_ЭЭ_Тариф],
    'ФАКТ' as [Тариф_Входящие_показатели_2],
    'FEB25' as [Дата],
    applymap('monthname_to_number', 'FEB25') as [Месяц _Тариф],
    [Тариф на покупку от сетей факт февраль] as [Параметры_Тариф]
    Resident tariff_fact_february;
    drop table tariff_fact_february;

    //--март
    tariff_fact_march:
    noconcatenate
    load
    [Тариф на покупку от сетей] as [Тариф на покупку от сетей факт март]
    FROM
    [$(Fact_reference_book_march_2025)]
    (ooxml, embedded labels, table is [$(Fact_reference_list)])
    where [Месторождение] = 'Западно-Салымское'
    and [Показатель ЭЭ 2] = 'Потребление электроэнергии МП';

    left join(Расчет_тарифа_temp_1)
    load
    'MAR25' as [Дата],
    applymap('monthname_to_number', 'MAR25') as [Месяц _Тариф],
    [Тариф на покупку от сетей факт март]
    Resident tariff_fact_march;

