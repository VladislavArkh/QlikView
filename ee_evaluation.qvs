    [Scenario],
    [Version],
    [Показатели профиль жидкости] as [Верхнесалымское_Добыча_нефти_в_поверхностных_условиях_тыс_м3],
    [Дата],
    applymap('monthname_to_number', [Дата]) as Месяц_эл_мощность
    Resident Профиль_жидкости
    where [ОП] = '(МР) Верхнесалымское'
    and [Профиль жидкости] = 'Добыча нефти в поверхностных условиях, тыс.м3';

    left join(Расчет_показателя_эл_мощность_temp_1)
    load
    [ОЕ УУ],
    [Scenario],
    [Version],
    [Дата],
    [Месяц_Натуральные_производственные_показатели] as [Месяц_эл_мощность],
