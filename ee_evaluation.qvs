//2.1. Ввод группы показателей «Натуральные производственные показатели»
sub Input_Group_NaturalProductionIndicators

    Натуральные_производственные_показатели_temp1:
    noconcatenate
    load
    [Количество дней],
    [Месяц] as Месяц_Натуральные_производственные_показатели
    from [$(Month_Book)]
    (ooxml, embedded labels, table is [$(Month_List)])
    where not isNull([Месяц]);

    //--Ваделыпское
    //--Месячные
    //Добыча нефти в поверхностных условиях, тыс.м3
    left join(Натуральные_производственные_показатели_temp1)
    load distinct
    [ОЕ УУ],
    [Scenario],
    [Version],
    [Показатели профиль жидкости] as [Ваделыпское_Добыча нефти в поверхностных условиях, тыс.м3],
    [Дата],
    applymap('monthname_to_number', [Дата]) as Месяц_Натуральные_производственные_показатели
    Resident Профиль_жидкости
    where [ОП] = '(МР) Ваделыпское'
    and [Профиль жидкости] = 'Добыча нефти в поверхностных условиях, тыс.м3';

    Натуральные_производственные_показатели_temp2:
    load distinct
    [ОЕ УУ],
    [Scenario],
    [Version],
    'Ваделыпское' as [Месторождение],
    'Добыча нефти в поверхностных условиях, тыс.м3' as [Показатель_ЭЭ_2_натуральные_производственные_показатели],
    [Дата],
    ([Ваделыпское_Добыча нефти в поверхностных условиях, тыс.м3] * 1000) as Показатели_натуральные_производственные,
    [Месяц_Натуральные_производственные_показатели]
    Resident Натуральные_производственные_показатели_temp1;

    //Добыча жидкости, тыс.м3
    left join(Натуральные_производственные_показатели_temp1)
    load distinct
    [ОЕ УУ],
    [Scenario],
    [Version],
    [Показатели ТЭП] as [Ваделыпское_Добыча жидкости, тыс.м3],
    [Дата],
    applymap('monthname_to_number', [Дата]) as Месяц_Натуральные_производственные_показатели
    Resident ТЭП_2
    where [Месторождение] = 'Ваделыпское'
    and [Входящие показатели ТЭП] = 'Добыча жидкости, тыс.м3';

    Concatenate(Натуральные_производственные_показатели_temp2)
    load distinct
    [ОЕ УУ],
    [Scenario],
