    sum([Параметры_потребление_ээ (план)]) as [Параметры_План_Прогноз]
    Resident
    Ваделыпское_Потребление_электроэнергии_difference_МП
    where [Scenario] <> 'FACT'
    group by [ОЕ УУ], [Version], [Scenario], [Дата];

    //факт
    concatenate(Показатели_ДН_9_temp_2)
    load
    [ОЕ УУ],
    [Scenario],
    [Version],
    'Всего потребление э.э' as [ПОКАЗАТЕЛИ],
    'Всего потребление э.э' as [ГРУППА ПОКАЗАТЕЛЕЙ],
    [Дата],
    sum([Параметры_потребление_ээ (факт)]) as [Параметры_Факт]
    Resident
    Ваделыпское_Потребление_электроэнергии_difference_МП
    where [Scenario] = 'FACT'
    group by [ОЕ УУ], [Version], [Scenario], [Дата];

