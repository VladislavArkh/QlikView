    load
    [Месяц] as [Месяц_эл_мощность],
    [Количество дней]
    from
    [$(Month_Book)]
    (ooxml, embedded labels, table is [$(Month_List)])
    where not isNull([Месяц]);

    left join(Расчет_показателя_эл_мощность_temp_1)
    load
    [ОЕ УУ],
    [Scenario],
    [Version],
    [Параметры_Группа_показателей_Удельный_расход_э_э] as [УРЭ транспорт жидкости  (ДНС-ДНС с УПСВ; ДНС-ЦПС)],
    [Дата],
    [Месяц_Группа_показателей_Удельный_расход_э_э] as [Месяц_эл_мощность]
    Resident Группа_показателей_Удельный_расход_э_э_report
    where
    [Показатель_2_ЭЭ_Удельный_расход] = 'УРЭ транспорт жидкости  (ДНС-ДНС с УПСВ; ДНС-ЦПС)';

    //Верхнесалымское
    left join(Расчет_показателя_эл_мощность_temp_1)
